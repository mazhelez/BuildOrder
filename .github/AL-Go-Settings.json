{
  "type": "PTE",
  "templateUrl": "https://github.com/freddydk/AL-Go@ALDoc",
  "CICDPushBranches": [
    "main",
    "release/*",
    "feature/*",
    "development"
  ],
  "CICDPullRequestBranches": [
    "main",
    "development"
  ],
  "DoNotUseCdnForArtifacts": true,
  "useProjectDependencies": true,
  "useCompilerFolder": true,
  "doNotPublishApps": true,
  "runs-on": "ubuntu-latest",
  "gitHubRunner": "ubuntu-latest",
  "excludeEnvironments": [
    "test"
  ],
  "alwaysBuildAllProjects": true,
  "ALDoc": {
    "ContinuousDeployment": true,
    "ExcludeProjects": [
      "test"
    ],
    "MaxReleases": 5,
    "Footer": "This is the footer",
    "Header": "Header",
    "DefaultIndexMD": "## This is the reference documentation\n\n{RELEASENOTES}"
  },
  "templateSha": "b1214e1a5af978d75e02883bc5942dd835e6734f"
}
